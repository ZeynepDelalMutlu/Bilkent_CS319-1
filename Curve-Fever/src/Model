package Model;

import java.awt.Color;
import java.awt.Point;

public class Player {

    private String name ="arsdtg";
    private Color color;
    private Point point;

    public Player(String name){
        this.name = name;
        this.color = color;
        this.point = point;
    }

    public String getName(){
        return name;
    }

    public void setName(String name){
        this.name = name;
    }

    public Color getColor(){
        return color;
    }

    public void setColor(Color color){
        this.color = color;
    }

    public String toString(){
        return name;
    }
}

